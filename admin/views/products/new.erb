<ul class="nav nav-tabs">
    <li> <%= link_to tag_icon(:list, pat(:list)), url(:products, :index) %></li>
    <li class="active"> <%= link_to tag_icon(:plus, pat(:new)), url(:products, :new) %></li>
</ul>
<div class="tabs-content">
    <% form_for :product, url(:products, :create), :multipart => true, :class => 'form-horizontal' do |f| %>
        <%= partial 'form', :locals => { :f => f } %>
    <% end %>
</div>